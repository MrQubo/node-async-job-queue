{
  "source": {
    "include": "src/"
  },
  "opts": {
    "destination": "docs/",
    "access": "public",
    "template": "node_modules/ink-docstrap/template/",
    "package": "package.json",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  },
  "recurseDepth": 0,
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "PromisePool",
    "footer": "",
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "https://",
    "methodHeadingReturns": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
